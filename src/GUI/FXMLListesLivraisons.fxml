<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXMLListesLivraisonsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Liste Livraison">
         <font>
            <Font size="26.0" />
         </font></Label>
      <Button fx:id="livraison" layoutX="14.0" layoutY="70.0" mnemonicParsing="false" onAction="#afficher" text="Afficher" />
      <TableView fx:id="tableLivraison" layoutY="108.0" prefHeight="293.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idlivraison" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="produit" prefWidth="98.0" text="Produit" />
            <TableColumn fx:id="quantite" prefWidth="136.0" text="Quantite" />
            <TableColumn fx:id="prix" prefWidth="101.0" text="Prix" />
            <TableColumn fx:id="date" minWidth="9.0" prefWidth="110.0" text="Date de livraison" />
            <TableColumn fx:id="id_client" prefWidth="75.0" text="Client" />
        </columns>
      </TableView>
      <Button fx:id="btnFaceuilr" layoutX="543.0" layoutY="14.0" mnemonicParsing="false" onAction="#retourAceuil" text="Aceuil" />
   </children>
</AnchorPane>
